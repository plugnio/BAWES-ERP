{
  "name": "bawes-erp",
  "version": "0.0.1",
  "description": "BAWES ERP Backend",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bawes/erp.git"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test DEBUG=false jest --maxWorkers=50%",
    "test:watch": "cross-env NODE_ENV=test DEBUG=false jest --watch --maxWorkers=50%",
    "test:cov": "cross-env NODE_ENV=test DEBUG=false jest --coverage --maxWorkers=50%",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "seed": "ts-node prisma/seed.ts",
    "seed:dev": "cross-env NODE_ENV=development prisma db seed",
    "seed:prod": "cross-env NODE_ENV=production prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "db:reset:dev": "cross-env NODE_ENV=development prisma migrate reset --force",
    "db:reset:prod": "cross-env NODE_ENV=production prisma migrate reset --force",
    "generate:sdk": "openapi-generator-cli generate -i swagger.json -g typescript-axios -o ./tmp-sdk",
    "build:sdk": "cd tmp-sdk && npm run build",
    "check:breaking-changes": "ts-node scripts/check-breaking-changes.ts",
    "permissions:list": "ts-node src/scripts/list-permissions.ts",
    "permissions:add": "ts-node src/scripts/add-permission.ts",
    "permissions:audit": "ts-node src/scripts/audit-permissions.ts",
    "permissions:assign-admin": "ts-node src/scripts/assign-admin-permissions.ts",
    "create:admin": "ts-node src/scripts/create-admin.ts"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.2.3",
    "@prisma/client": "^6.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/cookie-parser": "^1.4.8",
    "@types/passport-jwt": "^4.0.1",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^3.0.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "decimal.js": "^10.4.3",
    "ms": "^2.1.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prompts": "^2.4.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.12",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.17.9",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "openapi-diff": "^0.23.6",
    "prettier": "^3.0.0",
    "prisma": "^6.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "globalSetup": "<rootDir>/test/setup.ts",
    "globalTeardown": "<rootDir>/test/teardown.ts"
  }
}
